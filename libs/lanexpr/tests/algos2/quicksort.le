  fun rng_new(seed: int) : int =
  let
    var rng : int = fm_alloc(1)
  in
  (
    fmemset(rng, seed);
    rng
  )

  fun rng_free(rng: int) : void = fm_free(rng)

  fun rng_next(rng: int) : int =
  let
    var next : int = fmemget(rng)
  in
  (
    next = next * 1103515245 + 12345;
    fmemset(rng, next);
    (next / 65536) % 32768
  )










  fun sort(arr: int, len: int) : void =
  let

    fun swap(arr : int, i : int, j : int) : void =
    let
      var vi : int = fmemget(arr + i)
    in
    (
      fmemset(arr + i, fmemget(arr + j));
      fmemset(arr + j, vi)
    )

    fun partition(arr: int, beg: int, end: int) : int =
    let
      var pi : int = beg
      var pval : int = fmemget(arr + pi)
      var i : int = beg + 1
      var j : int = end - 1
      var cross : int = 0
    in
    (
      swap(arr, beg, pi);
      while (cross == 0) do (

        while (if (i < end) then fmemget(arr + i) < pval else 0) do (
          i = i + 1
        );
        while (if (j > beg) then fmemget(arr + j) > pval else 0) do (
          j = j - 1
        );

        if (i + 1 > j) then (
          cross = 1
        ) else (
          swap(arr, i, j)
        )
      );

      i = i - 1;
      swap(arr, beg, i);
      i
    )

    fun sort_rec(arr : int, beg: int, end: int) : void =
      if (end - beg > 1) then
      let
        var mid : int = partition(arr, beg, end)
      in
      (
        sort_rec(arr, beg, mid);
        sort_rec(arr, mid + 1, end)
      )

  in sort_rec(arr, 0, len)






  fun print_arr(arr: int, len: int) : void =
  let
    var i : int = 0
  in
  (
    putc(91);

    while (i < len)  do (
      print_int(fmemget(arr + i));
      if (i + 1 < len) then (
        putc(44);
        putc(32)
      );
      i = i + 1
    );

    putc(93);
    putc(10)
  )

